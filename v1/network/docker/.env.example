# Fabric Network Environment Configuration
# ============================================================================
# Copy this file to .env and modify credentials before deploying to production
# WARNING: Never commit .env files with real credentials to version control!
# ============================================================================

# Certificate Authority Admin Credentials
CA_ADMIN_USER=admin
CA_ADMIN_PASSWORD=adminpw

# CouchDB Credentials  
COUCHDB_USER=admin
COUCHDB_PASSWORD=adminpw

# Fabric Logging Level (DEBUG, INFO, WARNING, ERROR)
FABRIC_LOGGING_SPEC=INFO
